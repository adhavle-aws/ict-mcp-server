{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CloudFormationFullAccess",
            "Effect": "Allow",
            "Action": [
                "cloudformation:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "SSMParameterAccess",
            "Effect": "Allow",
            "Action": [
                "ssm:GetParameter",
                "ssm:GetParameters",
                "ssm:GetParametersByPath",
                "ssm:DescribeParameters",
                "ssm:PutParameter",
                "ssm:DeleteParameter",
                "ssm:AddTagsToResource",
                "ssm:RemoveTagsFromResource",
                "ssm:ListTagsForResource"
            ],
            "Resource": "*"
        },
        {
            "Sid": "EC2FullAccess",
            "Effect": "Allow",
            "Action": [
                "ec2:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "AutoScalingFullAccess",
            "Effect": "Allow",
            "Action": [
                "autoscaling:*",
                "autoscaling-plans:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ELBFullAccess",
            "Effect": "Allow",
            "Action": [
                "elasticloadbalancing:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "IAMFullAccess",
            "Effect": "Allow",
            "Action": [
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:GetRole",
                "iam:UpdateRole",
                "iam:PassRole",
                "iam:TagRole",
                "iam:UntagRole",
                "iam:ListRoles",
                "iam:ListRoleTags",
                "iam:AttachRolePolicy",
                "iam:DetachRolePolicy",
                "iam:PutRolePolicy",
                "iam:GetRolePolicy",
                "iam:DeleteRolePolicy",
                "iam:ListRolePolicies",
                "iam:ListAttachedRolePolicies",
                "iam:CreateInstanceProfile",
                "iam:DeleteInstanceProfile",
                "iam:GetInstanceProfile",
                "iam:AddRoleToInstanceProfile",
                "iam:RemoveRoleFromInstanceProfile",
                "iam:ListInstanceProfiles",
                "iam:ListInstanceProfilesForRole",
                "iam:CreateServiceLinkedRole",
                "iam:DeleteServiceLinkedRole",
                "iam:GetServiceLinkedRoleDeletionStatus",
                "iam:CreatePolicy",
                "iam:DeletePolicy",
                "iam:GetPolicy",
                "iam:GetPolicyVersion",
                "iam:ListPolicyVersions",
                "iam:CreatePolicyVersion",
                "iam:DeletePolicyVersion",
                "iam:SetDefaultPolicyVersion",
                "iam:UpdateAssumeRolePolicy"
            ],
            "Resource": "*"
        },
        {
            "Sid": "LambdaFullAccess",
            "Effect": "Allow",
            "Action": [
                "lambda:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "APIGatewayFullAccess",
            "Effect": "Allow",
            "Action": [
                "apigateway:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "DynamoDBFullAccess",
            "Effect": "Allow",
            "Action": [
                "dynamodb:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "S3FullAccess",
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "RDSFullAccess",
            "Effect": "Allow",
            "Action": [
                "rds:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "CloudWatchFullAccess",
            "Effect": "Allow",
            "Action": [
                "cloudwatch:*",
                "logs:*",
                "application-autoscaling:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "SNSSQSFullAccess",
            "Effect": "Allow",
            "Action": [
                "sns:*",
                "sqs:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "CognitoFullAccess",
            "Effect": "Allow",
            "Action": [
                "cognito-idp:*",
                "cognito-identity:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "SecretsManagerFullAccess",
            "Effect": "Allow",
            "Action": [
                "secretsmanager:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "KMSFullAccess",
            "Effect": "Allow",
            "Action": [
                "kms:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Route53FullAccess",
            "Effect": "Allow",
            "Action": [
                "route53:*",
                "route53resolver:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "CloudFrontFullAccess",
            "Effect": "Allow",
            "Action": [
                "cloudfront:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ContainerServicesFullAccess",
            "Effect": "Allow",
            "Action": [
                "ecs:*",
                "ecr:*",
                "eks:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "StepFunctionsEventBridgeFullAccess",
            "Effect": "Allow",
            "Action": [
                "states:*",
                "events:*",
                "scheduler:*",
                "pipes:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ElastiCacheFullAccess",
            "Effect": "Allow",
            "Action": [
                "elasticache:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "WAFSecurityFullAccess",
            "Effect": "Allow",
            "Action": [
                "wafv2:*",
                "waf:*",
                "waf-regional:*",
                "shield:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ACMFullAccess",
            "Effect": "Allow",
            "Action": [
                "acm:*",
                "acm-pca:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "BedrockFullAccess",
            "Effect": "Allow",
            "Action": [
                "bedrock:*",
                "bedrock-agentcore:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VPCNetworkingFullAccess",
            "Effect": "Allow",
            "Action": [
                "vpc-lattice:*",
                "networkmanager:*",
                "directconnect:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "AppConfigFullAccess",
            "Effect": "Allow",
            "Action": [
                "appconfig:*",
                "servicediscovery:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "CodeServicesFullAccess",
            "Effect": "Allow",
            "Action": [
                "codebuild:*",
                "codepipeline:*",
                "codecommit:*",
                "codedeploy:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "AnalyticsFullAccess",
            "Effect": "Allow",
            "Action": [
                "kinesis:*",
                "firehose:*",
                "athena:*",
                "glue:*",
                "es:*",
                "opensearch:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "MiscServicesFullAccess",
            "Effect": "Allow",
            "Action": [
                "backup:*",
                "resource-groups:*",
                "tag:*",
                "servicequotas:*",
                "support:*",
                "health:*",
                "xray:*"
            ],
            "Resource": "*"
        }
    ]
}
